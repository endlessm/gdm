msgstr "%s: Аввалияти сервер ба %d таъин карда нашуд: %s"

#: ../daemon/gdm-server.c:684
#, c-format
msgid "%s: Empty server command for display %s"
msgstr "%s: Фармони холии сервер барои дисплейи %s"

